			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">G80.1</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">G80.2</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">M21.0</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">M21.2</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">M21.4</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">M21.5</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">M21.9</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">Q68.1</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">Q72.5</SPAN></FONT>,
			<FONT COLOR="#106bbe"><SPAN STYLE="font-weight: normal">Q72.6</SPAN></FONT>,
